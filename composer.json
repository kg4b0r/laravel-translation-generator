{
  "name": "procyonweb/laravel-translation-generator",
  "description": "Translation generator for Laravel, Blade and Vue",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Gabor Koszegi",
      "email": "gabor@procyonweb.lu"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0",
    "illuminate/console": "^6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "ProcyonWeb\\TranslationGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProcyonWeb\\TranslationGenerator\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ProcyonWeb\\TranslationGenerator\\ServiceProvider"
      ]
    }
  }
}
